                    //alert('Good to see you, ' + response.name + '.');
                    try {
                        $.ajax({url:
                                'http://ogapp.heroku.com/story/persist_user?uid=' + response.uid + '&name=' + response.name + "&access_token=" + response.authResponse.accessToken});
                    } catch (e) {
                        alert("Trying to post back: " + e);
                    }
                });
            } else {
                alert('Something went wrong!');
            }
        });
    };

    // Load the SDK Asynchronously
    (function(d){
